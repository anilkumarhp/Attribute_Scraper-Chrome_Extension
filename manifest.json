{
    "manifest_version": 2,
    "name": "Hello World",
    "version":"1.0",
    "description": "A hello world extension",
    "browser_action": {
        "default_popup":"popup.html"
    },
    "background": {
        "scripts": ["jquery-3.3.1.min.js","popup.js"],
        "persistent": false
      },
      "permissions": ["tabs"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js":["jquery-3.3.1.min.js","content.js"]        
    }]

}